;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Projectile",
"language": "GDScript",
"path": "res://Projectiles/Projectile.gd"
}, {
"base": "KinematicBody2D",
"class": "SlideMover",
"language": "GDScript",
"path": "res://Player/SlideMover.gd"
}, {
